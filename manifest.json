{
  "manifest_version": 2,
  "name": "Passwall",
  "version": "1.0.0",
  "browser_action": {
    "default_title": "Passwall",
    "default_popup": "./index.html",
    "default_icon": "./logo.png"
  },
  "permissions": [
    "webRequest",
    "<all_urls>",
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": [
      "./bgscript.js"
    ]
  }
}